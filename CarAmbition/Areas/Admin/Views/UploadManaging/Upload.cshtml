@{
    ViewBag.Title = "Upload";
    Layout = "~/Areas/Admin/Views/AdminLayout/AdminLayout.cshtml";
}


@using (@Html.BeginForm("Image", "UploadManaging", FormMethod.Post))
{
    <div>
        @Html.TextBox("uploadTxt", "", htmlAttributes: new { @class = "userTextBox", @placeholder = "Nhập tên xe cần tìm...   " })
        <button type="submit" id="submitBtn" class="btn btn-default" style="margin-top:-9px; height:30px">Tìm</button>
    </div>
}


